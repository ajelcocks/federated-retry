{
    "version": "0.2.0",
    "configurations": [
        {
          "name": "Crome",
          "request": "launch",
          "type": "chrome",
          "url": "http://localhost:3102",
          "webRoot": "${workspaceFolder}",
          "sourceMaps": true,
          "sourceMapPathOverrides": {
            "webpack:///*": "${webRoot}/src/*"
          },
          "userDataDir": false,
          "runtimeArgs": [
            "--profile-directory=React"
          ]
      }
  ]
}
